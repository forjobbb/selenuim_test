<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="automation" verbose="10" parallel="tests" thread-count="3">

    <!--    HOW TO RUN MANY SPECIFIC METHODS USING EXCLUDE-->
    <test name="tests">
        <classes>
            <class name="com.build.qa.build.selenium.tests.BuildTest">
                <methods>
                    <exclude name="navigateToHomePage"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>